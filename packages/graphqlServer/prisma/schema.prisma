// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String
}

model Calendar {
  day_id       String @id
  year         Int
  month        Int
  day          Int
  quarter      Int
  day_of_week  Int
  day_of_year  Int
  week_of_year Int
}
